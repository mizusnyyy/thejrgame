script/source = "extends Control

@onready var play_button = $Graj_button
@onready var settings_button = $ustawienia_button
@onready var quit_button = $quit_button

func _ready():
	play_button.pressed.connect(_on_play_pressed)
	settings_button.pressed.connect(_on_settings_pressed)
	quit_button.pressed.connect(_on_quit_pressed)

func _on_play_pressed():
	print(\"Kliknięto GRAJ\")
	get_tree().change_scene_to_file(\"res://scenes/story/pre-core/house.tscn\")

func _on_settings_pressed():
	print(\"Kliknięto USTAWIENIA\")
	get_tree().change_scene_to_file(\"res://scenes/story/pre-core/house.tscn\")

func _on_quit_pressed():
	print(\"Kliknięto WYJDŹ\")
	get_tree().quit()
"

